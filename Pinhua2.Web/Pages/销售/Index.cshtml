@page
@model Pinhua2.Web.Pages.销售.IndexModel
@{
    ViewData["Title"] = "Index";
}

<pre>
    @(Get<tb_报价表, tb_报价表D>())
</pre>

@functions{
    public string Get<TMain, TDetail>
    () where TMain : _BaseTableMain where TDetail : _BaseTableDetail
    {
        return JsonConvert.SerializeObject(new
        {
            A = pinhua2.Set<TMain>
            ().ToList(),
            B = pinhua2.Set<TDetail>
                ().ToList()
        }, Formatting.Indented);
    }
}
