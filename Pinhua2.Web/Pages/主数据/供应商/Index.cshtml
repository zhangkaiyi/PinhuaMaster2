@page
@model Pinhua2.Web.Pages.主数据.供应商.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].往来号)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].简称)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].全称)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].联系人)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].联系电话)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].地址)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.供应商列表[0].类型)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.供应商列表)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.往来号)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.简称)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.全称)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.联系人)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.联系电话)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.地址)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.类型)
                </td>
                <td>
                    <a asp-page="./Edit" asp-route-id="@item.RecordId">Edit</a> |
                    <a asp-page="./Details" asp-route-id="@item.RecordId">Details</a> |
                    <a asp-page="./Delete" asp-route-id="@item.RecordId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
