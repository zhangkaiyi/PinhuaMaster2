@page
@model Pinhua2.Web.Pages.主数据.字典.DetailsModel

@{
    ViewData["Title"] = "Details";

    var templateModel = new _CRUD_Template_Model
    {
        RecordMain = new _CRUD_Template_Model_Main
        {
            Title = "字典",
            Data = Model.字典,
        },
        RecordDetailsArray = new List<_CRUD_Template_Model_Details>
{
            new _CRUD_Template_Model_Details
            {
                Title="明细",
                Data = Model.字典D列表.Cast<object>(),
            },
        }
    };
}

<partial name="_CRUD_Template/_Details" model="templateModel" />

<div class="mb-3">
    <a asp-page="./Edit" asp-route-id="@Model.字典.RecordId">修改</a> |
    <a asp-page="./Index">返回列表</a>
</div>